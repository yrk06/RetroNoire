config_version=4

_global_script_classes=[ {
"base": "",
"class": "FmodNative",
"language": "NativeScript",
"path": "res://addons/fmod/Fmod.gdns"
}, {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
} ]
_global_script_class_icons={
"FmodNative": "",
"GitAPI": ""
}

[application]

config/name="RetroNoire"
run/main_scene="res://testScenes/TestEnv.tscn"
boot_splash/image="res://assets/Splash Retro.png"
config/icon="res://icon.png"

[autoload]

UiInterface="*res://UI/UIInterface.gd"
PlayerInterface="*res://Player/PlayerInterface.gd"
Game="*res://Game.gd"
Fmod="*res://addons/fmod/Fmod.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "LogicConectors", "fmod" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

